pico-8 cartridge // http://www.pico-8.com
version 18
__lua__
-- sinusoidal intro
-- by uka/smr 2020

debug = 1
cnt,cg,coy,sxl,sxh = 0,0,0,0,0
crackname="sinusoidal intro"
scroll="yeah! this is another production from ukasw titled: ~~~ "..crackname.." ~~~ . time to write something bigger and useful.. no more intros! greetings!              +++ msx and code by uka/smr 29.03.2020 +++"
bckc,bc,txtc = 1,12,10
txtt={}

function dat2str()
 local o=(cg-10)
 for i=1, #txtt do
  local x,y=txtt[i][1],txtt[i][2]
  pset(x,y/2-(sin((x-88)/100))*y*0.35,9)
  pset(x*3,(y-60)*2+y+11,7)
  pset(x,y*2-(sin((x-38)/100))*y*0.35,0)
 end
end
--
function str2dat()
 txtt={}
 for y=60,65 do
  for x=0,127 do
   if(pget(x,y)==txtc) add(txtt,{x,y})
  end 
 end
end
-- use globals: coy, cg
function draw_ball()
 circfill(64,64+coy,cg,bc)
 circ(64,64+coy,cg+2,0)
end
-- use globals: sxh,sxl,scroll
function draw_scroll()
 local stxt = sub(scroll,1+sxh,34+sxh) 
 print(stxt,-sxl,60,txtc)
 print(stxt,-sxl,90,0)
end
--
function draw_sign()
 local x,y,c=123,123,5
 rectfill(x-1,y,x+4,y+4,0)
 line(x,y+1,x,y+3,7)
 line(x+2,y+1,x+2,y+3,7)
 pset(x+1,y+3,txtc)
 pset(x+4,y+3,txtc)
end
--
function draw_lines()
 rectfill(1,0,2,127,0)
 rectfill(125,0,126,127,0)
 rectfill(4,0,4,127,0)
 rectfill(123,0,123,127,0)
end
-- show time and mem/cpu usage
function _debug()
 rectfill(0,0,127,5,0)
 print("mem/cpu/t: "..stat(1).." "..stat(2).." "..t(),0,0,7)
end
-------------------------------
-- main
-------------------------------
function _init()
 for i=1,60 do scroll=" "..scroll end
 music(0)
end

function _update()
 -- counters
 local ti=t()
 cnt+=1
 if(cnt>360) cnt=1
 cg=cos((t()*2.5)*0.65)*10+20
 coy=cos((t()*2.5)*0.65)*20
 -- scroll
 if(ti>4) sxl+=1
 if(sxl>3)then 
  sxl=0
  sxh+=1
  if(sxh>#scroll) sxh=0
 end
end

function _draw()
 cls(bckc)
 draw_scroll()
 str2dat()
 draw_lines()
 dat2str()
 draw_ball()
 draw_sign()
 if(debug==1) _debug()
end

