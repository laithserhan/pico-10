-- show time and mem/cpu usage
function _debug()
 rectfill(0,0,127,5,1)
 print("mem/cpu/t: "..stat(1).." "..stat(2).." "..t(),0,0,7)
end
--
function str2dat(str)
 local dat={}
 cls()
 print(str,0,1,1)
 for y=1,6 do
  dat[y]={}
  for x=0,4*#str do
   if(pget(x,y)==1)add(dat[y],x)
  end
 end
 dat[0]=#str
 return(dat)
end
-- use globals: sxh,sxl,scroll,scrollc
function draw_scroll()
 print(sub(scroll,1+sxh,34+sxh),-sxl,111,7)
 for i=1,5 do
  local py=110+i
  for j=1,128 do
   local px=j-1
   local p=pget(px,py)
   if(p!=0)then
    pset(px,i*2+117,scrollc[i])
   end
  end
  rectfill(0,py,127,py,0)
 end
end
-- use globals: rasterc
function draw_rasterbars()
 local step=1
 if(rm<#rasterc) step=2
 for i=1,rm,step do  
  local ya,yb,c=i+8,101-i,rasterc[i]
  --line(0,ya,127,ya,c)
  --line(0,yb,127,yb,c)
  rectfill(0,ya,127,ya,c)
  rectfill(0,yb,127,yb,c)
 end
end
--
function draw_pxs(y,dat,ox,c) 
 if(ox==nil) ox=0
 local w=dat[0]
 local x=64-w*2
 if(c!=nil) x=x+2
 for i=1,#dat do
  if(c!=nil and i<#dat)then
   yl=i-1+y
   if(yl<128)then
    local xa,xb=x-4+i*ox,w*4+x+2+i*ox
    --line(xa,yl,xb,yl,c)
    rectfill(xa,yl,xb,yl,c)
   end
  end
  for j=1,#dat[i] do
   pset(dat[i][j]+i*ox+x,i-1+y,7)     
  end
 end
end
-- use globals: tile, gp, lox, cnt, tm
function draw_logo()
 for ly=1,10 do
  for ty=1,6 do
   local y=ty+(ly-1)*6
   local tox=0
   if(t()>14) tox=sin((cnt+y*1.2)*0.025)*2.5  
   for tx=1,tm do
    local c=tile[ty][tx]    
    for lx=1,#gp[1] do
     local x=tx+(lx-1)*6 
     if(gp[ly][lx]==1)then
      pset(lox+tox+x+6,y+24,c)
     end
    end  
   end
  end
 end
end
--
function draw_sign()
 local x,y,c=123,125,5
 rectfill(x-1,y,x+4,y+3,0)
 line(x,y,x,y+2,c)
 line(x+2,y,x+2,y+2,c)
 pset(x+1,y+2,c)
 pset(x+4,y+2,c)
end
--
function draw_stars()
 local star="âœ½"
 for i=1,#stars do
  print(star,stars[i][1],stars[i][2],stars[i][4])
 end
end
--
function draw_vhs()
 for i=0,127 do
  if(flr(rnd(2))==1)then
   local px=pget(i,by)
   pset(i,by,px+6)
  end
  local pl=pget(i,by+1)
  pset(i-1,by+1,pl)
  pl=pget(i,by+2)
  pset(i-1,by+2,pl)
 end
end
-------------------------------
-- main
-------------------------------
function _init()
 uptextd=str2dat(uptext)
 cracknamed=str2dat(crackname)
 local r=flr(rnd(5)+1)
 cracknamec=cc[r]
 scrollc=sc[r]
 rasterc=rc[r]
 for i=1,60 do scroll=" "..scroll end
 for i=#rasterc,1,-1 do add(rasterc,rasterc[i]) end
 music(0)
end

function _update()
 -- counters
 local ti=t()
 cnt+=1
 if(cnt>360) cnt=1

 -- vhs
 by+=1
 if(by>256) by=0

 -- uptext and crackname
 col+=1
 if(col>#cracknamec) col=1
 if(upy<0) upy+=1
 if(cny>106) cny-=1
 
 -- rasterbars
 if(ti>2 and rm<#rasterc) rm+=1
 
 -- scroll
 if(ti>4) sxl+=1
 if(sxl>3)then 
  sxl=0
  sxh+=1
  if(sxh>#scroll) sxh=0
 end
 
 -- g*p
 if(ti>6)then
  if(tm<6) tm+=0.5
 end
 if(ti>23.3) lox=cos((t()*1.5)*0.25)*40
 
 -- stars
 for i=1,#stars do
  if(stars[i][1]<-5 and ti>30)then
   stars[i][1]=128
   stars[i][2]=20+i*8--40+10*flr(rnd(4))
   stars[i][3]=4+flr(rnd(10))
   stars[i][4]=flr(rnd(15))
  else
   stars[i][1]=stars[i][1]-stars[i][3]
  end
 end 
end

function _draw()
 cls()
 draw_pxs(upy,uptextd,1)
 draw_pxs(cny,cracknamed,-1,cracknamec[col])
 draw_rasterbars()
 draw_logo()
 draw_scroll()
 draw_stars()
 draw_sign()
 draw_vhs()
 if(debug==1) _debug()
end

