--
function draw_balls()
 local x=10-box*0.5
 local y=8-boy*0.5
 print("●",x,y,8)
 pset(box+45-box*0.2,48+y*0.15,8)
 pset(box+205-box*0.2,28+y*0.15,8)
end
-- use globals: eyesbmp, tilesbmp, box
function draw_bmp(y)
 for i=1,#eyesbmp do
  local py=y+(i-1)*4
  for j=1,#eyesbmp[i] do
   local t=eyesbmp[i][j]
   if(t>0) draw_pxs((j-1)*4+box,py,tilesbmp[t])
  end
 end
end
-- 
function draw_pxs(x,y,dt)
 for i=1,#dt do  
  local yp=y+i-1
  for j=1,#dt[i]-bo do
   if(dt[i][j]==1)then
    pset(x+j-1,yp,c)
    pset(x+j-1,yp+2,c)
   end 
  end
 end
end
--
function draw_sign()
 local x,y=123,125
 line(x,y,x,y+2,c)
 line(x+2,y,x+2,y+2,c)
 pset(x+1,y+2,c)
 pset(x+4,y+2,c)
end
-- use globals scroll, sxh, sxl
function draw_scroll(y)
 print(sub(scroll,1+sxh,34+sxh),-sxl,118,c)
 rectfill(0,y,lo,y,c-1)
 rectfill(128-lo,y+14,127,y+14,c-1) 
 for i=1,5 do
  local py=117+i
  for j=1,64 do
   local px=j-1
   local p=pget(px,py)
   if(p==c)then
    pset(px*2,i*2+y+1,c)
   end
  end
  rectfill(0,py,127,py,bckc)
 end
end
-- show time and mem/cpu usage
function _debug()
 rectfill(0,0,127,5,1)
 print("mem/cpu/t: "..stat(1).." "..stat(2).." "..t(),0,0,7)
end
-------------------------------
-- main
-------------------------------
function _init()
 for i=1,20 do scroll=" "..scroll end
 music(0)
end

function _update()
 -- counters
 ti=t()
 cnt+=1
 if(cnt>360) cnt=1

 -- scroll
 if(ti>10)then
  sxl+=1
  if(sxl>3)then 
   sxl=0
   sxh+=1
   if(sxh>#scroll) sxh=0 
  end
 end
 -- bmp
 box=cos((t()*1.5)*0.20)*96-96
 boy=sin((t()*1.5)*0.60)*80-80
 if(ti>9 and bo>0) bo-=1
 
 -- text
 if(ti>2 and to<160) to+=2
 tox=cos((t()*1.5)*0.80)*(160-to)
 if(ti<10 and lo<128) lo+=2
end

function _draw()
 cls(bckc)
 rectfill(0,1,lo,1,c-1)
 rectfill(128-lo,99,127,99,c-1) 
 if(ti>2) print(txt,64-(#txt/2*4)+tox,102,7)
 draw_bmp(3)
 draw_scroll(109)
 draw_sign()
 draw_balls()
 if(debug==1) _debug()
end
